#include<bits/stdc++.h>
using namespace std;

int main()
{
        /*
        *  Write your code here. 
         *  Read input as specified in the question.
         *  Print output as specified in the question.
        */
        int n;
        cin>>n;
        int t1=0;
        int t2=1;
        int t3;
        if(n<=1){
                cout<<n;
        }else{
            for(int i=2; i<=n;i++){
             t3=t1+t2;
             t1=t2;
             t2=t3;  
        }
        cout<< t3;
        }
        
}
